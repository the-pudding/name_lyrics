	<link inline rel='stylesheet' href='bundle.css' />

	{{> base/font}}


	<script inline src='head.js'></script>

	{{> base/analytics }}

</head>

<body>

	<main id='content'>
		{{> story/sidebar }}

		<article class='content__article'>
